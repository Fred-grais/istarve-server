<h1>Listing user_favorite_lists</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Restaurant</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_favorite_lists.each do |user_favorite_list| %>
      <tr>
        <td><%= user_favorite_list.user_id %></td>
        <td><%= user_favorite_list.restaurant_id %></td>
        <td><%= link_to 'Show', user_favorite_list %></td>
        <td><%= link_to 'Edit', edit_user_favorite_list_path(user_favorite_list) %></td>
        <td><%= link_to 'Destroy', user_favorite_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User favorite list', new_user_favorite_list_path %>
